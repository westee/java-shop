<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <!-- 使用驼峰命名法转换字段。 -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="OrderInfo" type="com.wester.api.data.OrderInfo" />
        <typeAlias alias="GoodsInfo" type="com.wester.api.data.GoodsInfo" />
    </typeAliases>

    <mappers>
        <mapper resource="db/mybatis/MyOrderMapper.xml" />
        <mapper resource="db/mybatis/OrderMapper.xml" />
        <mapper resource="db/mybatis/OrderGoodsMapper.xml" />
    </mappers>

</configuration>